#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,i,target;
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    scanf("%d",&target);
    int found=0;
    for(i=0;i<n;i++)
    {
        if(arr[i]==target)
        {
            printf("Door Number is %03d-DN\n",i);
            found=1;
            break;
            
        }
    }
    if(!found)
    {
        printf("-1\n");
        
    }
    return 0;
}
