#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,sum;
    scanf("%d",&n);
    if(n<100)
        { 
        printf("Invalid Input");
        }
    else
        {
        while(n!=0)
     {
            sum+=n%10;
        n=n/10;
        }
        printf("Sum of digit is %d",sum);
        }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
